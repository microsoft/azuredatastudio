					"**/{vs,sql}/platform/*/common/**",
					"**/{vs,sql}/editor/common/**",
					"**/{vs,sql}/editor/contrib/*/common/**",
					"**/{vs,sql}/workbench/common/**",
					"**/{vs,sql}/workbench/services/*/common/**",
