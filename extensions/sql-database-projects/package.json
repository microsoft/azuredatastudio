    "xml-formatter": "2.1.0"
  },
  "devDependencies": {
    "@microsoft/vscodetestcover": "^1.2.2",
    "@microsoft/azdata-test": "^3.0.4",
    "@types/fs-extra": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/request": "^2.48.8",
    "@types/semver": "^7.3.1",
    "@types/sinon": "^9.0.4",
    "@types/which": "^2.0.1",
    "@types/xml-formatter": "^1.1.0",
    "mocha": "^9.2.2",
    "should": "^13.2.1",
    "sinon": "^9.0.2",
