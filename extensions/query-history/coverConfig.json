{
	"enabled": true,
	"relativeSourcePath": "..",
	"relativeCoverageDir": "../../coverage",
	"ignorePatterns": [
		"**/node_modules/**",
		"**/test/**",
		"main.js"
	],
	"reports": [
		"cobertura",
		"lcov",
		"json"
	],
	"verbose": false,
	"remapOptions": {
		"basePath": "..",
		"useAbsolutePaths": true
	}
}
