                <string>keyword.control.kusto</string>
            </dict>
            <dict>
                 <!-- User-defined functions
                    -  https://docs.microsoft.com/en-us/azure/kusto/query/functions/user-defined-functions
				-->
                <key>match</key>
                <string>(?<=let ).+(?=\W*=)</string>
