        <source xml:lang="en">Connect</source>
        <target state="translated">连接</target>
      </trans-unit>
      <trans-unit id="azure.resource.connectsqldb.title">
        <source xml:lang="en">Add to Servers</source>
        <target state="translated">添加到服务器</target>
      </trans-unit>
