      </trans-unit>
      <trans-unit id="cms.connectionOptions.userName.displayName">
        <source xml:lang="en">User name</source>
        <target state="translated">ユーザー名</target>
      </trans-unit>
      <trans-unit id="cms.connectionOptions.userName.description">
