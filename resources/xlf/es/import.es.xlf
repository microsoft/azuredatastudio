        <source xml:lang="en">Modify Columns</source>
        <target state="translated">Modificar columnas</target>
      </trans-unit>
      <trans-unit id="flatFileImport.page4Name">
        <source xml:lang="en">Summary</source>
        <target state="translated">Resumen</target>
      </trans-unit>
